%ul{:id => 'slides'}
    - f = @photosRes.first
    %li
        .div=  f[:photo].title
        .div=link_to(image_tag(f[:photo].image.url(:medium)), photo_comments_path(f[:photo].id))

    - @photosRes.each do |p|
        %li
            .div=  p[:photo].title
            .div=link_to(image_tag(p[:photo].image.url(:medium)), photo_comments_path(p[:photo].id))
